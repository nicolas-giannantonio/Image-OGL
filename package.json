{
  "name": "oglimage",
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./types": "./types/index.d.ts"
  },
  "files": [
    "src/**/*",
    "types/**/*"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "ogl": "^1.0.3"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
